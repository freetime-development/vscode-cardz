{
	"name": "vscode-cardz",
	"displayName": "vscode-cardz",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:studyNotes",
		"onCommand:studyNotes.info",
		"onCommand:studyNotes.newCard"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "study-notes",
					"title": "Study Notes",
					"icon": "media/img/dep.svg"
				}
			]
		},
		"views": {
			"study-notes": [
				{
					"id": "studyNotes",
					"name": "Study Notes"
				}
			]
		},
		"commands": [
			{
				"command": "studyNotes.info",
				"title": "Info",
				"category": "Study Notes Info"
			},
			{
				"command": "studyNotes.stats",
				"title": "Card stats",
				"category": "Study Note stats"
			},
			{
				"command": "studyNotes.newCard",
				"title": "New Card",
				"category": "Cards"
			}
		],
		"menus": {
			"view/item/context": [
				{
					"command": "studyNotes.info",
					"when": "view == studyNotes && viewItem == studyNote"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test",
		"test:unit": "mocha -r ts-node/register src/test/unit/**/*.test.ts  --exit"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"mocha": "^6.2.0",
		"ts-node": "^8.3.0",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"webpack": "^4.39.1",
		"webpack-cli": "^3.3.6"
	},
	"dependencies": {
		"@types/cheerio": "^0.22.13",
		"@types/markdown-it": "0.0.8",
		"@types/mustache": "^0.8.32",
		"cheerio": "^1.0.0-rc.3",
		"markdown-it": "^10.0.0",
		"mustache": "^3.1.0"
	}
}
