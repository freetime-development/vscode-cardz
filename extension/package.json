{
	"name": "vscode-cardz",
	"displayName": "vscode-cardz",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:studyNotes",
		"onCommand:studyNotes.webView"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
			  {
				"id": "study-notes",
				"title": "Study Notes",
				"icon": "media/img/dep.svg"
			  }
			]
		},
		"views": {
			"study-notes": [
				{
					"id": "studyNotes",
					"name": "Study Notes"
				}
			]
		},
		"commands": [
			{
				"command": "studyNotes.webView",
				"title": "Study Notes",
				"category": "Study Notes"
			  }
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"webpack": "^4.39.1",
		"webpack-cli": "^3.3.6"
	}
}
